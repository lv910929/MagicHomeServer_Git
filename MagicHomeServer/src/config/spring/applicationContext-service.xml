<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:aop="http://www.springframework.org/schema/aop"
	xmlns:context="http://www.springframework.org/schema/context" xmlns:tx="http://www.springframework.org/schema/tx"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.2.xsd  
        http://www.springframework.org/schema/aop http://www.springframework.org/schema/aop/spring-aop-3.2.xsd  
        http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-3.2.xsd  
        http://www.springframework.org/schema/tx http://www.springframework.org/schema/tx/spring-tx-3.2.xsd"
	default-autowire="byName">
	<!-- Account -->
	<bean id="accountService" class="com.lencity.magichome.service.AccountService"
		scope="prototype">
	</bean>

	<!-- AdminUser -->
	<bean id="adminUserService" class="com.lencity.magichome.service.AdminUserService"
		scope="prototype">
	</bean>

	<!-- Device -->
	<bean id="deviceService" class="com.lencity.magichome.service.DeviceService"
		scope="prototype">
	</bean>

	<!-- Feedback -->
	<bean id="feedbackService" class="com.lencity.magichome.service.FeedbackService"
		scope="prototype">
	</bean>

	<!-- Repair -->
	<bean id="repairService" class="com.lencity.magichome.service.RepairService"
		scope="prototype">
	</bean>

	<!-- Software -->
	<bean id="softwareService" class="com.lencity.magichome.service.SoftwareService"
		scope="prototype">
	</bean>

	<!-- Domain -->
	<bean id="domainService" class="com.lencity.magichome.service.DomainService"
		scope="prototype">
	</bean>

	<!-- Hardware -->
	<bean id="hardwareService" class="com.lencity.magichome.service.HardwareService"
		scope="prototype">
	</bean>

	<!-- Repair_Progress -->
	<bean id="repairProgressService" class="com.lencity.magichome.service.RepairProgressService"
		scope="prototype">
	</bean>

	<!-- AlertInfo -->
	<bean id="alertInfoService" class="com.lencity.magichome.service.AlertInfoService"
		scope="prototype">
	</bean>

	<!-- Log -->
	<bean id="logService" class="com.lencity.magichome.service.LogService"
		scope="prototype">
	</bean>

	<!-- File -->
	<bean id="documentService" class="com.lencity.magichome.service.DocumentService"
		scope="prototype">
	</bean>

	<!-- ClientInfo -->
	<bean id="clientInfoService" class="com.lencity.magichome.service.ClientInfoService"
		scope="prototype">
	</bean>

	<bean id="deviceNumService" class="com.lencity.magichome.service.DeviceNumService"
		scope="prototype">
	</bean>

</beans>
